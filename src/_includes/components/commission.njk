---
layout: "layouts/base.njk"
---

<article class="commission">
    <h1>{{ title }}</h1>
    <hr>
        {% include 'components/carousel.njk' %}
        {{ content | safe }}
        <h3>Prices</h3>
        <table class="prices">
            {% for category, price in prices %}
                <tr class="price">
                    <td>{{ category }}:</td>
                    <td>{{ price }}&euro; </td>
                </tr>
            {% endfor %}
        </table>
    </article>