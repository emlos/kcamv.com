<article class="container commission">
    <a href="{{ type.url }}">
        <h1>{{ type.data.title }}</h1>
    </a>
    <hr>
        <img src="{{ type.data.previewImage }}" alt="{{ type.data.title }} preview image">
            <table class="prices">
                {% for category, price in prices %}
                    <tr class="price">
                        <td>{{ category }}:</td>
                        <td>{{ price }}&euro;
                        </td>
                    </tr>
                {% endfor %}
            </table>
        </article>